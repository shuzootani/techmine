{
  "name": "tech-mine",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "fetch-jsonp": "^1.1.3",
    "node-sass-chokidar": "^0.0.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-scripts": "1.0.12",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.6.0",
    "superagent-jsonp": "^0.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
